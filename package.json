{
  "name": "pomjs",
  "version": "1.0.2",
  "description": "Module Solution base on Vue ",
  "main": "index.js",
  "scripts": {
    "example-dev":
      "cross-env VUE_ENV=server node --debug example/src/index.js ",
    "example-build":
      "cross-env BUILD=true node example/src/index.js && babel example/src -d example/dist",
    "server":
      "cross-env VUE_ENV=server NODE_ENV=production node example/dist/index.js",
    "build": "babel src -d dist"
  },
